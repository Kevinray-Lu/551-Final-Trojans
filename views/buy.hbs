		<!--gird men start-->
		<div class="grid-menu-area">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
						<div class="gird-menu">
							<ul>
								<li><a href="/">Home</a> <i class="fa fa-angle-right"></i></li>
								<li>Buy</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--gird men start-->
		<!--shop-area start-->
		<div class="shop-area">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
						{{!-- former widget area --}}
					</div>
					<div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
						<div class="shop-list-toolbar">
						</div>
						<!--shop-gird view-->
						<div class="row">
							<div class="show-gird-product another">
								<ol>
									{{#each shop}}
									<div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
										<div class="single-product">
											<div class="product-img">
												<a class="b-s-p-img" href="/buy/{{id}}">
													<img src="data:{{img.contentType}};base64,{{img.data}}">
												</a>
												<div class="img-block">
													<div class="primary-icon">
														<a href="/buy/{{id}}"><i class="fa fa-random"></i></a>
														<a href="/buy/{{id}}"><i class="fa fa-eye"></i></a>
													</div>
													<div class="hover-icon">
													<span class="random"><a href="/buy/{{id}}"><i class="fa fa-random"></i></a></span>
														<span class="eye"><a href="/buy/{{id}}"><i class="fa fa-eye"></i>Quick view</a></span>
													</div>    
												</div>
											</div>
											<div class="product-block-text">
												<h3><a href="/buy/{{id}}">{{title}}<span>Price:{{price}}</span></a></h3>
												<p><small>{{description}}</small></p>
												{{!-- <button type="submit">buy now</button> --}}
												<a class="p-b-t-a-c" href="/buy/{{id}}">view more</a>
												{{!-- <input type="submit" value="Add"> --}}
											</div>
										</div>    
									</div>
									{{/each}}
								</ol>								
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--shop-area end-->
		<!--our brand start-->
	<div class="our-brand-area owl-indicator">
